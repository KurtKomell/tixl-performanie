    {
      "OutputId": "8d9c0b1a-2f3e-4145-6789-abcdef012345"/*LastReceivedClientDictionary*/,
      "Position": {
        "X": 300.0,
        "Y": 700.0
      },
      "Description": "The last message received from any client, parsed as a JSON dictionary (key-value pairs). Only valid when ClientMessageParsingMode is set to JsonKeyValue."
    },
    {
      "OutputId": "9c0b1a2f-3e4d-4056-789a-bcdef0123456"/*ClientMessageWasTrigger*/,
      "Position": {
        "X": 300.0,
        "Y": 800.0
      },
      "Description": "A trigger that becomes true for one frame each time a new message is received from any client."
    }
  ]
}